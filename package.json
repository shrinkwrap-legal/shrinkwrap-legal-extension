{
  "name": "shrinkwrap-legal",
  "version": "0.1.1",
  "description": "shrinkwrap.legal browser extension",
  "main": "index.js",
  "scripts": {
    "watch": "cross-env NODE_ENV=development TARGET_BROWSER=chrome webpack --config webpack/webpack.dev.js --watch",
    "watch-remote": "cross-env NODE_ENV=remote TARGET_BROWSER=chrome webpack --config webpack/webpack.dev.js --watch",
    "watch-remote-firefox": "cross-env NODE_ENV=remote TARGET_BROWSER=firefox webpack --config webpack/webpack.dev.js --watch",
    "build": "cross-env TARGET_BROWSER=chrome webpack --config webpack/webpack.prod.js",
    "build-firefox": "cross-env TARGET_BROWSER=firefox webpack --config webpack/webpack.prod.js",
    "generate-api": "swagger-typescript-api generate --path http://localhost:8080/api/v3/api-docs -o ./src/api -n generated.ts  --extract-enums --generate-union-enums",
    "generate-api-remote": "swagger-typescript-api generate --path https://api.shrinkwrap.legal/api/v3/api-docs -o ./src/api -n generated.ts  --extract-enums --generate-union-enums",
    "clean": "rimraf dist",
    "test": "npx jest",
    "style": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "author": "",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/shrinkwrap-legal/shrinkwrap-legal-extension.git"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.64.1",
    "@tanstack/react-query-devtools": "^5.64.1",
    "bootstrap": "5.3.8",
    "react": "^18.2.0",
    "react-bootstrap": "2.10.10",
    "react-bootstrap-icons": "1.11.6",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.297",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/webextension-polyfill": "0.12.3",
    "autoprefixer": "10.4.21",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "7.1.2",
    "dotenv-webpack": "^8.1.0",
    "glob": "11.0.3",
    "jest": "^29.7.0",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "prettier": "3.2.4",
    "resolve-url-loader": "5.0.0",
    "rimraf": "6.0.1",
    "sass": "1.63.6",
    "sass-loader": "16.0.5",
    "style-loader": "4.0.0",
    "swagger-typescript-api": "^13.1.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^8.0.0",
    "typescript": "^5.3.3",
    "webextension-polyfill": "0.12.0",
    "webpack": "^5.76.0",
    "webpack-cli": "6.0.1",
    "webpack-merge": "^5.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true
  }
}
